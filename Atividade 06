class Program
{
    static void Main(string[] args)
    {
        int num1, num2;

        Console.WriteLine("Digite o primeiro numero inteiro");
        num1 = Convert.ToInt32(Console.ReadLine());

        Console.WriteLine("Digite o segundo numero inteiro");
        num2 = Convert.ToInt32(Console.ReadLine());

        if (num1 < num2)
        {
                int contador = num1;
                Console.WriteLine("\nNúmeros inteiros entre " + num1 + " e " + num2 + " :");
            do
            {
                Console.WriteLine(contador);
                contador++;
            } while (contador <= num2);
        }
        else
        {
                int contador = num2;
                Console.WriteLine("\nNúmeros inteiros entre " + num2 + " e " + num1 + " :");
            do
            {
                Console.WriteLine(contador);
                contador++;
            } while (contador <= num1);
        }
    }
}
